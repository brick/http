{
    "name": "brick/http",
    "description": "HTTP request and response library",
    "type": "library",
    "keywords": [
        "Brick",
        "HTTP",
        "Request",
        "Response"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "php-coveralls/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Http\\Tests\\": "tests/"
        }
    }
}
